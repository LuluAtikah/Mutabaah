<?xml version="1.0" encoding="utf-8"?>
<!--Custom report.-->
<openerp>
<data>
    <report
        id="report_amal"
        model="mutabaah.mutabaah"
        string="Amal Report"
        name="mutabaah.report_view"
        file="mutabaah.report"
        report_type="qweb-pdf"/>

<template id="report_document">
    <t t-call="report.external_layout">
        <div class="page">
            <div class="row">
                <h3>Amalan Yaumiyah</h3>
            </div>
        </div>
    </t>
</template>

<template id="report">
    <t t-call="report.html_container">
        <t t-foreach="docs" t-as="doc">
            <t t-raw="translate_doc(doc_id, doc_model, 'partner_id.lang', 'account.report_without_prices_document')"/>
        </t>
    </t>
</template>
</data>
</openerp>